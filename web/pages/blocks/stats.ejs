<script src="/libs/js/morris.min.js"></script>
<script src="/libs/js/datepicker.js"></script>
<link rel="stylesheet" type="text/css" href="/libs/css/morris.css">
<link rel="stylesheet" type="text/css" href="/libs/css/datepicker.css">
